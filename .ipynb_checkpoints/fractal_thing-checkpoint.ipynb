   "source": [
    "import turtle\n",
    "def square():\n",
    "    turtle.begin_fill()\n",
    "    for i in range(4):\n",
    "        turtle.forward(20)\n",
    "        turtle.right(90)\n",
    "    turtle.end_fill()\n",
    "\n",
    "turtle.color(\"blue\")\n",
    "turtle.fillcolor(\"blue\")\n",
    "square"
   ]
